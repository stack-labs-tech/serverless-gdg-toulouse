  "scripts": {
    "sls": "AWS_PROFILE=stack sls deploy",
    "build:prod": "webpack --mode=production --config webpack.config.prod.js",
    "predeploy": "yarn build:prod",
    "deploy": "AWS_PROFILE=stack aws s3 sync ./dist s3://serverless-stack-labs-coffee-talk-cls",
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "test": "jest",
    "predocker:build": "yarn build:prod",
    "docker:build": "docker build -t coffee-talk-serverless:latest .",
    "docker:run": "docker run -p 8082:80 coffee-talk-serverless:latest",
    "invoke": "AWS_PROFILE=stack serverless invoke local --function translator --path ./templates/mocks/translator.json"
  },
  "dependencies": {
    "@material-ui/core": "^3.5.1",
